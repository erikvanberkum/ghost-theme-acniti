<!DOCTYPE html>
<html lang="nl">
<head>

    {{!-- Document Settings --}}
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>

    {{!-- Base Meta --}}
    <title>waterstof voor sporters en gezondheid</title>
    <meta name="HandheldFriendly" content="True"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

    {{!-- Styles'n'Scripts --}}
    <link rel="stylesheet" type="text/css" href="{{asset "main/css/main.css"}}"/>

    {{!-- This tag outputs SEO meta+structured data and other important settings --}}
    {{ghost_head}}

</head>
<body class="is-preload {{body_class}}">

<div id="wrapper">
    <main id="main">
        <div class="inner">

            {{!-- Header --}}
            <header id="header">
                {{#if @site.logo}}
                    <a href="{{@site.url}}" class="logo image"><img src="{{@site.logo}}" alt="{{@site.title}}"/></a>
                {{else}}
                    <a href="{{@site.url}}" class="logo">{{@site.title}}</a>
                {{/if}}
                <ul class="icons">
                    <li><a href="https://shop.acniti.com" class="icon fa-shopping-cart" target="_blank" title="Kopen"><span class="label">Kopen</span></a>
                    </li>
                </ul>
            </header>

            {{!-- Featured Post --}}
            {{#get "posts" filter="featured:true" limit="1" as |featured_post|}}
                {{#foreach featured_post}}
                    <article id="banner">
                        <div class="content">
                            <header>
                                <h1><a href="{{url}}">{{title}}</a></h1>
                                {{#if custom_excerpt}}<p>{{custom_excerpt}}</p>{{/if}}
                            </header>
                            {{content words="42"}}
                            <ul class="actions">
                                <li><a href="{{url}}" class="button big">Meer</a></li>
                            </ul>
                        </div>
                        {{#if feature_image}}
                            <a href="{{url}}" class="image object">
                                <img src="{{img_url feature_image}}" alt="{{title}}"/>
                            </a>
                        {{/if}}
                    </article>
                {{/foreach}}
            {{/get}}
            <section>
                <header class="major">
                    <h2>Producten</h2>
                </header>
                <section class="posts">
                    {{#foreach posts}}
                        <article>
                            {{#if feature_image}}
                                <a href="{{url}}" class="image fit"><img src="{{img_url feature_image}}"
                                                                         alt="{{title}}"/></a>
                            {{/if}}
                            <h3><a href="{{url}}">{{title}}</a></h3>
                            <p>{{excerpt words="33"}}</p>
                            <ul class="actions special">
                                <li><a href="{{url}}" class="button">Meer</a></li>
                            </ul>
                        </article>
                    {{/foreach}}
                </section>

            </section>
        </div>
    </main>

    <div id="sidebar">
        <div class="inner">

            {{!-- menu structure for dutch language --}}
            <nav id="menu">
                <header class="major">
                    <h2>menu</h2>
                </header>
                <ul class="links">
                    <li class="nav-{{slug}}"><a href="{{@site.url}}/nl/">Start</a></li>
                    <li class="nav-{{slug}}"><a href="{{@site.url}}/nl/respiro-cubic-waterstoftherapie/">Respiro</a>
                    </li>
                    <li class="nav-{{slug}}"><a href="{{@site.url}}/nl/sedis-waterstof-water-dispenser/">Sedis waterstof
                        dispensor</a></li>
                    <li class="nav-{{slug}}"><a href="{{@site.url}}/nl/sedis-waterstof-water-dispenser-met-inhalator/">Sedis
                        Respiro waterstof dispenser met inhaler</a></li>
                    <li class="nav-{{slug}}"><a href="{{@site.url}}/nl/magna-waterstofdispenser/">Magna
                        waterstofdispenser</a></li>
                    <li class="nav-{{slug}}"><a href="{{@site.url}}/over-ons/">over ons</a></li>
                    <li class="nav-{{slug}}"><a href="{{@site.url}}/prijzen/">prijzen</a></li>
                </ul>
            </nav>

            {{!-- acniti contact --}}
            <section>
                <header class="major">
                    <h2>Neem contact op</h2>
                </header>
                <div class="mini-posts">
                    <article>
                        <h3>acniti LLC</h3>
                        <p>1-2-9 Nyoidani<br>
                            Minoh Osaka<br>
                            ã€’562-0011<br>
                            Japan<br>
                            <br>
                            <a href="mailto: h2@acniti.com">h2@acniti.com</a><br>
                            <a href="https://www.acniti.com">www.acniti.com</a></p>
                    </article>
                </div>
            </section>

            {{!-- Contact --}}
            {{#get "posts" slug="contact"as |contact|}}
                {{#if contact}}{{#foreach contact}}
                    <section>
                        <header class="major">
                            <h2>{{title}}</h2>
                        </header>
                        {{content}}
                    </section>
                {{/foreach}}{{/if}}
            {{/get}}

            {{!-- Footer --}}
            <footer id="footer">
                    <ul class="icons">
                        {{#if @site.twitter}}
                            <li><a href="{{twitter_url @site.twitter}}" class="icon fa-twitter" title="Twitter"><span
                                    class="label">Twitter</span></a></li>
                        {{/if}}
                        <li><a href="https://shop.acniti.com" class="icon fa-shopping-cart" title="Buy"><span class="label">Buy</span></a>
                        </li>
                        <li><a href="https://www.linkedin.com/company/acniti/" class="icon fa-linkedin" title="acniti linkedin"><span class="label">linkedin</span></a>
                        <li><a href="https://feedly.com/i/subscription/feed/{{@site.url}}/rss/" class="icon fa-rss"
                               target="_blank" rel="noopener"><span class="label" title="RSS">RSS</span></a></li>
                    </ul>
                <p class="copyright">&copy; {{@site.title}} LLC - Alle rechten voorbehouden</p>
                <p class="design">Design: <a href="https://html5up.net" target="_blank" rel="noopener">HTML5 UP</a></p>
            </footer>

        </div>
    </div>
</div>


{{!-- Theme Scripts --}}
<script src="https://s3.acniti.com/h2/assets/main/js/jquery.min.js"></script>
<script src="https://s3.acniti.com/h2/assets/main/js/browser.min.js"></script>
<script src="https://s3.acniti.com/h2/assets/main/js/breakpoints.min.js"></script>
<script src="https://s3.acniti.com/h2/assets/main/js/util.js"></script>
<script src="https://s3.acniti.com/h2/assets/main/js/main.js"></script>

{{!-- Ghost outputs important scripts & data with the {ghost_foot} tag
it should always be the very last thing before the closing body tag --}}
{{ghost_foot}}

</body>
</html>



